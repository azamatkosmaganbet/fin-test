{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".", // Убедитесь, что у вас есть этот параметр
    "paths": {
      "@/*": ["src/*"] // Убедитесь, что этот путь корректно указывает на ваш проект
    },
    "jsx": "react-jsx"
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
